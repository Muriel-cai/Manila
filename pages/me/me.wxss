.userInfo{
  width:710rpx;
  height: 160rpx;
  background: #fff;
  margin: 10px auto;
}
.picBox{
  width:120rpx;
  height: 120rpx;
  float:left;
  padding: 20rpx;
  border-radius: 50%;
}
.textInfo{
  width:440rpx;
  height: 120rpx;
  padding: 20rpx;
  float:left;
}
.username{
  padding-top: 10rpx;
  height: 50rpx;
  line-height: 50rpx;
  color: #333;
  font-size: 16px;
}
.phoneNum{
  height: 50rpx;
  line-height: 50rpx;
  color: #666;
  font-size: 14px;
}

.sellBox{
  position: relative;
  width:710rpx;
  height: auto;
  text-align: center;
  margin: 0 auto;
}
.sellItem{
  position: relative;
  width:335rpx;
  height:280rpx;
  display: block; 
  margin:0 40rpx 30rpx 0; 
  float: left;
  background: #fff;
}

.no-right{
  margin-right:0px;
}
.sellItem image{
  margin-left: 86rpx;
  margin-top: 40rpx;
}
.sellItem text{
  height: 60rpx;
  font-size: 14px;
  line-height: 60rpx;
}
.hasnum{
  position: absolute;
  z-index: 5;
  top:0;
  right:0;
  

}
.showewmPics{
  position:fixed;
  z-index: 10;
  top:0;
  bottom:0;
  left:0;
  right: 0;
  width:100%;
  height: 100%;
}
.opcit{
  position: fixed;
  z-index: 10;
  top:0;
  bottom:0;
  left:0;
  right: 0;
  width:100%;
  height: 100%;
  background: rgba(0,0,0,0.5);
}
.ewmBox{
  position: relative;
  background: #fff;
  z-index: 50;
  width:600rpx;
  height: 700rpx;
  padding:10rpx 20rpx ;
  top:50%;
  margin-top: -336rpx;
  left:50%;
  margin-left: -320rpx;
  text-align: center;
  vertical-align: middle;
  line-height: 400rpx;
  
}
.tostText{
  position: absolute;   
  left:0px;
  z-index:60;
  display:block;
  width:640rpx;
  text-align:center;
  line-height:30px;
  top: 20rpx;
  font-size: 14px;
}
.toseting{
  position: absolute;   
  left:0px;
  z-index:60;
  display:block;
  width:640rpx;
  text-align:center;
  line-height:30px;
  bottom: 20rpx;
  font-size: 14px;
}